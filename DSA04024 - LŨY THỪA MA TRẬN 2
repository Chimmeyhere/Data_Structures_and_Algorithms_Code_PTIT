#include <bits/stdc++.h>
using namespace std;
struct data
{
    long long x[10][10];
};
long long mod = 1000000007;
struct data xxx(struct data a, struct data b, int n)
{
    struct data xx;
    for (int i = 0; i < n; i++)
    {
        for (int j = 0; j < n; j++)
        {
            xx.x[i][j] = 0;
            for (int k = 0; k < n; k++)
            {
                xx.x[i][j] = (xx.x[i][j] + (a.x[i][k] * b.x[k][j]) % mod) % mod;
            }
        }
    }
    return xx;
};
struct data poww(struct data a, long long k, int n)
{
    if (k == 1)
        return a;
    if (k % 2 == 1)
        return xxx(a, poww(a, k - 1, n), n);
    struct data xx = poww(a, k / 2, n);
    return xxx(xx, xx, n);
}
int main()
{
    int t;
    cin >> t;
    while (t--)
    {
        int n;
        long long k, s = 0;
        cin >> n >> k;
        struct data a;
        for (int i = 0; i < n; i++)
            for (int j = 0; j < n; j++)
                cin >> a.x[i][j];
        a = poww(a, k, n);
        for (int i = 0; i < n; i++)
            s = (s + a.x[i][n - 1]) % mod;
        cout << s << endl;
    }
}
